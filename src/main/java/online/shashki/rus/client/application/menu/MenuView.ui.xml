<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
        >

    <ui:with field="res" type="online.shashki.rus.client.resources.AppResources"/>
    <ui:with field="vars" type="online.shashki.rus.client.resources.Variables"/>

    <g:HTMLPanel ui:field="panel">
        <b:Navbar ui:field="navbar" position="FIXED_TOP" height="{vars.navbarTopHeight}">
            <b:Container>
                <b:NavbarBrand ui:field="brand" href="#!home">
                </b:NavbarBrand>
                <b:NavbarNav ui:field="navLeft" addStyleNames="{res.style.xLargeFont}"/>
                <b:NavbarNav ui:field="navRight" pull="RIGHT" addStyleNames="{res.style.largeFont}"/>
            </b:Container>
        </b:Navbar>
    </g:HTMLPanel>
</ui:UiBinder>
