<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel ui:field="panel">
        <g:HTML ui:field="id"/>
        <g:HTML ui:field="playEndDate"/>
        <g:HTML ui:field="whoDidWin"/>
        <b:Image ui:field="endGameScreenshot"/>
        <g:HTML ui:field="whitePlayerName"/>
        <g:HTML ui:field="blackPlayerName"/>
        <b:Button ui:field="playGame" icon="PLAY_CIRCLE" addStyleNames="btn-link"/>
    </g:HTMLPanel>
</ui:UiBinder>
