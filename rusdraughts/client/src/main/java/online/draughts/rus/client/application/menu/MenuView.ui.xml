<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
        >

    <ui:with field="res" type="online.draughts.rus.client.resources.AppResources"/>
    <ui:with field="vars" type="online.draughts.rus.client.resources.Variables"/>

    <g:HTMLPanel ui:field="panel">
        <b:Navbar ui:field="navbar" position="FIXED_TOP">
            <b:Container>
                <b:NavbarHeader addStyleNames="{res.style.navbarHeader}">
                    <b:NavbarBrand ui:field="brand" href="#!home"/>
                    <b:NavbarCollapseButton dataTarget="#navbar-collapse"/>
                </b:NavbarHeader>
                <b:NavbarCollapse b:id="navbar-collapse">
                    <b:NavbarNav ui:field="navLeft" addStyleNames="{res.style.xLargeFont}"/>
                    <b:NavbarNav ui:field="navRight" pull="RIGHT" addStyleNames="{res.style.largeFont}"/>
                </b:NavbarCollapse>
            </b:Container>
        </b:Navbar>
    </g:HTMLPanel>
</ui:UiBinder>
